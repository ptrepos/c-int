      <definition>void mg_uint256_right_shift_small(
	const mg_uint256 *op1, 
	int shift, 
	/*out*/mg_uint256 *ret);</definition>
      <parameter name="op1">
        <description>ビットシフトを行う値</description>
      </parameter>
      <parameter name="shift">
        <description>シフトさせる値(0~31)</description>
      </parameter>
      <parameter name="ret">
